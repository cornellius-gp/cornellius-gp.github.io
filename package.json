{
  "name": "cornellius-gp.github.io",
  "homepage": "https://cornellius-gp.github.io",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@fortawesome/fontawesome": "^1.1.3",
    "@fortawesome/fontawesome-free-brands": "^5.0.6",
    "@fortawesome/fontawesome-free-solid": "^5.0.6",
    "@fortawesome/react-fontawesome": "0.0.17",
    "bootstrap": "^4.0.0",
    "gh-pages": "^1.2.0",
    "highlight.js": "^9.12.0",
    "katex": "^0.9.0",
    "npm-run-all": "^4.1.2",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-scripts": "^1.1.4",
    "react-snapshot": "^1.3.0",
    "reactstrap": "^6.1",
    "sass": "^1.55.0"
  },
  "scripts": {
    "build-css": "sass -I ./src -I ./node_modules src/:src/",
    "watch-css": "npm run build-css && sass -I ./src -I ./node_modules src/:src/ --watch",
    "start-js": "react-scripts start",
    "build-js": "react-scripts build",
    "start": "npm-run-all -p watch-css start-js",
    "build": "npm-run-all build-css build-js && react-snapshot",
    "predeploy": "npm run build",
    "deploy": "gh-pages -b master -d build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "reactSnapshot": {
    "include": [
      "/index.html"
    ],
    "snapshotDelay": 300
  }
}
